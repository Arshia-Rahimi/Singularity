import com.github.singularity.core.shared.PluginSettings;

CREATE TABLE Plugins (
    name TEXT PRIMARY KEY NOT NULL UNIQUE,
    options TEXT AS PluginSettings NOT NULL
);

index:
SELECT * FROM Plugins;

insert:
INSERT INTO Plugins (name, options)
    VALUES (?, ?);

update:
UPDATE Plugins
    SET options = ?
    WHERE name == ?;
