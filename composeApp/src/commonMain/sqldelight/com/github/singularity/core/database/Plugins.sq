CREATE TABLE Plugins (
    name TEXT PRIMARY KEY NOT NULL UNIQUE,
    is_enabled INTEGER NOT NULL CHECK (is_enabled IN (0, 1)),
    plugin_data TEXT NOT NULL
);

index:
SELECT *
    FROM Plugins;

getPlugin:
SELECT *
    FROM Plugins
    WHERE name == ?
    LIMIT 1;

insert:
INSERT INTO Plugins (name, is_enabled, plugin_data)
    VALUES (?, ?, ?);

update:
UPDATE Plugins
    SET is_enabled = ?, plugin_data = ?
    WHERE name == ?;

delete:
DELETE FROM Plugins
    WHERE name = ?;
