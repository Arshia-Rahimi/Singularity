import com.github.singularity.core.syncservice.plugin.PluginOptions;

CREATE TABLE Plugins (
    name TEXT PRIMARY KEY NOT NULL UNIQUE,
    options TEXT AS PluginOptions NOT NULL
);

index:
SELECT * FROM Plugins;

getPlugin:
SELECT *
    FROM Plugins
    WHERE name == ?
    LIMIT 1;

insert:
INSERT INTO Plugins (name, options)
    VALUES (?, ?);

update:
UPDATE Plugins
    SET options = ?
    WHERE name == ?;

delete:
DELETE FROM Plugins
    WHERE name = ?;
