import com.github.singularity.core.shared.PluginOptions;

CREATE TABLE Plugins (
    name TEXT PRIMARY KEY NOT NULL UNIQUE,
    options TEXT AS PluginOptions NOT NULL
);

index:
SELECT * FROM Plugins;

insert:
INSERT INTO Plugins (name, options)
    VALUES (?, ?);

update:
UPDATE Plugins
    SET options = ?
    WHERE name == ?;
